{
  "AppName": "WebScrapper",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    }
  },
  "MaxExecutionTime": { //Milliseconds
    "Images": {
      "Get": 60000
    }
  },
  "AllowedHosts": "*"
}
